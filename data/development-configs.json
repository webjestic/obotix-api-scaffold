[
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "common",
        "logish": {
            "level": "trace",
            "performanceTime": true,
            "controllers": [
              {
                "name": "console",
                "active": true,
                "displayOnlyEnvNamespace": false,
                "displayLevels": [
                  "trace",
                  "debug",
                  "info",
                  "warn",
                  "error",
                  "fatal"
                ],
                "format": "%level %namespace %entry %performance",
                "useColor": true
              },
              {
                "name": "file",
                "active": false,
                "files": [
                  {
                    "title": "app logs",
                    "active": false,
                    "writeLevels": [
                      "warn",
                      "error",
                      "fatal"
                    ],
                    "format": "[%datetime %level] %namespace %host - %entry %performance",
                    "filename": "logs/app.log",
                    "maxsize_in_mb": 5,
                    "backups_kept": 5,
                    "gzip_backups": false
                  }
                ]
              }
            ]
        }
    },
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "gateway",
        "redis": {},
        "logish": {"level": "trace"}
    },
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "auth",
        "redis": {},
        "logish": {"level": "trace"},
        "roles": {
            "basic": 0,
            "moderator": 2,
            "manager": 4,
            "admin": 8
        }
    },
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "admin",
        "redis": {},
        "logish": {"level": "trace"}
    },
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "pay",
        "redis": {},
        "logish": {"level": "trace"}
    },
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "vue"
    },
    {
        "CONFIG_ENV": "development",
        "CONFIG_APP": "mobile"
    }
]